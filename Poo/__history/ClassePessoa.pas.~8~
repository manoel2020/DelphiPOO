unit ClassePessoa;

interface

type
   TPessoa = class

   private

   public
      nome:string;
      DataNasc: integer;
      sexo: string;
      etnia: string;
      function Idade : integer;
   end;
implementation
function TPessoa.Idade : integer;
begin
   Result := Trunc((now - StrToDate(DataNasc)))
end;

end.
